package com.recalot.common.communication;

import java.util.HashMap;

/**
 * @author Matthaeus.schmedding
 */
public class User implements com.recalot.common.interfaces.communication.User {

    private String id;
    private HashMap<String, Object> map;

    public User(String id, HashMap<String, Object> map){
        this.id = id;
        this.map = map;
    }

    @Override
    public String getId() {
        return id;
    }

    @Override
    public HashMap<String, Object> getAll() {
        return map;
    }

    @Override
    public Object getValue(String key) {
        return map.get(key);
    }
}
