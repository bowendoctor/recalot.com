package com.recalot.common.exceptions;

/**
 * @author Matth√§us Schmedding (info@recalot.com)
 */
public class MissingArgumentException extends BaseException {

    public MissingArgumentException() {
        super();
    }

    public MissingArgumentException(String message) {
        super(message);
    }

    public MissingArgumentException(String format, String arg) {
        super(format, arg);
    }

    public MissingArgumentException(String format, String arg1, String arg2) {
        super(format, arg1, arg2);
    }

    public MissingArgumentException(String format, String arg1, String arg2, String arg3) {
        super(format, arg1, arg2);
    }

    public MissingArgumentException(String format, String arg1, String arg2, String arg3, String arg4) {
        super(format, arg1, arg2, arg3, arg4);
    }

    public MissingArgumentException(Throwable cause) {
        super(cause);
    }
}
