package com.recalot.common.impl.template;

import java.io.InputStream;
import java.nio.charset.Charset;

/**
 * @author matthaeus.schmedding
 */
public class DefaultTemplateResult implements com.recalot.common.interfaces.template.TemplateResult {
    private int status;
    private String contentType;
    private InputStream inputStream;
    private Charset charset;

    public DefaultTemplateResult(int status, String contentType, InputStream inputStream, Charset charset) {
        this.status = status;
        this.contentType = contentType;
        this.inputStream = inputStream;
        this.charset = charset;
    }

    @Override
    public int getStatus() {
        return status;
    }

    @Override
    public String getContentType() {
        return contentType;
    }

    @Override
    public InputStream getResult() {
        return inputStream;
    }

    @Override
    public Charset getCharset() {
        return charset;
    }
}
