package com.recalot.common.communication;

import java.util.Date;
import java.util.HashMap;

/**
 * @author Matthaeus.schmedding
 */
public class Interaction implements com.recalot.common.interfaces.communication.Interaction {

    private String id;
    private String userId;
    private String itemId;
    private Date timeStamp;
    private String type;
    private HashMap<String, Object> content;

    public Interaction(String id, String userId, String itemId, Date timeStamp, String type, HashMap<String, Object> content) {
        this.id = id.intern();
        this.userId = userId.intern();
        this.itemId = itemId.intern();
        this.timeStamp = timeStamp;
        this.type = type.intern();
        this.content = content;
    }


    @Override
    public String getId() {
        return id;
    }

    @Override
    public String getUserId() {
        return userId;
    }

    @Override
    public String getItemId() {
        return itemId;
    }

    @Override
    public Date getTimeStamp() {
        return timeStamp;
    }

    @Override
    public String getType() {
        return type;
    }

    @Override
    public HashMap<String, Object> getAll() {
        return content;
    }

    @Override
    public Object getValue(String key) {
        return content.get(key);
    }
}
